{
  "name": "hyperliquid-prx",
  "version": "1.6.2",
  "description": "SDK for Hyperliquid API - fork from https://github.com/nomeida/hyperliquid",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/browser.js",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "browser": {
    "./dist/index.js": "./dist/browser.js"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "prepare": "npm run build",
    "test:node": "node examples/node-test.js",
    "test:browser": "npx serve -s . -p 3000",
    "test:all": "npm run build && npm run test:node && echo '\nTo test in browser, run: npm run test:browser and visit http://localhost:3000/examples/browser-test.html'"
  },
  "keywords": [
    "hyperliquid",
    "api",
    "sdk",
    "rest",
    "websocket",
    "trading",
    "crypto",
    "blockchain",
    "arbitrum",
    "dex"
  ],
  "author": "Nomeida",
  "license": "MIT",
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0-beta2",
    "axios": "^1.7.9",
    "axios-retry": "^4.5.0",
    "dotenv": "^16.4.7",
    "ethers": "^6.13.2",
    "fs": "^0.0.1-security",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.6",
    "path": "^0.12.7",
    "url": "^0.11.4"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "jest": "^29.0.0",
    "serve": "^14.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rjoshig/hyperliquid-prx"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}